/* rubik-900 - latin */
@font-face
  font-family 'Rubik'
  font-style normal
  font-weight 900
  src local('Rubik Black'), local('Rubik-Black'),
       url('assets/font/rubik-v2-latin-900.woff2') format('woff2'), 
       url('assets/font/rubik-v2-latin-900.woff') format('woff')

