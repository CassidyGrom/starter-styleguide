{
  "name": "starter",
  "version": "1.1.0",
  "description": "Starter template for Polygraph stories",
  "scripts": {
    "dist:webpack": "webpack --config webpack.config.dist.js",
    "dist:inline": "inline-source --compress false --root dist dist/index.html dist/index.html",
    "dist": "mkdir -p dist && npm run dist:webpack && npm run dist:inline",
    "start": "webpack-dev-server --config webpack.config.dev.js"
  },
  "author": "Russell Goldenberg",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-1": "^6.13.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.24.0",
    "dsv-loader": "^1.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "inline-source-cli": "^1.1.1",
    "json-loader": "^0.5.4",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.15.0"
  },
  "dependencies": {
    "d3": "^4.2.2"
  }
}
